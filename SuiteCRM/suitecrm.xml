<?xml version="1.0"?>
<Container version="2">
  <Name>SuiteCRM</Name>
  <Repository>bitnami/suitecrm:latest</Repository>
  <Registry>https://hub.docker.com/r/bitnami/suitecrm/</Registry>
  <Network>bridge</Network>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://forums.unraid.net/topic/104242-help-with-suitecrm-how-can-i-deploy/</Support>
  <Project>https://suitecrm.com/</Project>
  <Overview>SuiteCRM is a free open-source Customer Relationship Management application for servers that is written in PHP.</Overview>
  <Category>Productivity:</Category>
  <WebUI>http://[IP]:[PORT:80]/</WebUI>
  <TemplateURL>https://gitea.fuckedup.fr/Kryckely-Home-Unraid-Server/SuiteCRM/raw/branch/master/suitecrm.xml</TemplateURL>
  <Icon>https://gitea.fuckedup.fr/Kryckely-Home-Unraid-Server/SuiteCRM/raw/branch/master/suitecrm.png</Icon>
  <Description>SuiteCRM is a free open-source Customer Relationship Management application for servers that is written in PHP.</Description>
  <Config Name="Port" Target="8080" Default="8000" Mode="tcp" Description="Port" Type="Port" Display="always" Required="true" Mask="false">8000</Config>
  <Config Name="SUITECRM_USERNAME" Target="SUITECRM_USERNAME" Default="user" Mode="" Description="SuiteCRM application username" Type="Variable" Display="always" Required="true" Mask="false">user</Config>
  <Config Name="SUITECRM_PASSWORD" Target="SUITECRM_PASSWORD" Default="bitnami" Mode="" Description="SuiteCRM application password" Type="Variable" Display="always" Required="true" Mask="false">bitnami</Config>
  <Config Name="SUITECRM_DATABASE_HOST" Target="SUITECRM_DATABASE_HOST" Default="Hostname or IP" Mode="" Description="Database Hostname or IP" Type="Variable" Display="always" Required="true" Mask="false">Hostname or IP</Config>
  <Config Name="SUITECRM_DATABASE_PORT_NUMBER" Target="SUITECRM_DATABASE_PORT_NUMBER" Default="3306" Mode="" Description="Database Port" Type="Variable" Display="always" Required="true" Mask="false">3306</Config>
  <Config Name="SUITECRM_DATABASE_USER" Target="SUITECRM_DATABASE_USER" Default="root" Mode="" Description="SUITECRM_DATABASE_USER" Type="Variable" Display="always" Required="true" Mask="false">root</Config>
  <Config Name="SUITECRM_DATABASE_PASSWORD" Target="SUITECRM_DATABASE_PASSWORD" Default="password" Mode="" Description="SUITECRM_DATABASE_PASSWORD" Type="Variable" Display="always" Required="true" Mask="false">password</Config>
  <Config Name="SUITECRM_DATABASE_NAME" Target="SUITECRM_DATABASE_NAME" Default="suitecrm" Mode="" Description="SUITECRM_DATABASE_NAME" Type="Variable" Display="always" Required="true" Mask="false">suitecrm</Config>
  <Config Name="Appdata_backup" Target="/bitnami/suitecrm_backup" Default="/mnt/user/appdata/suitecrm/" Mode="rw" Description="Appdata_backup" Type="Path" Display="always" Required="true" Mask="false">/mnt/user/appdata/suitecrm/</Config>
</Container>

